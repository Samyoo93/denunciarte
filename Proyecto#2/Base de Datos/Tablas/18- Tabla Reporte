CREATE TABLE Reporte
(
     reporte_id NUMBER(6)
          CONSTRAINT reporte_id_nn NOT NULL
          CONSTRAINT reporte_id_pk PRIMARY KEY,
     descripcion VARCHAR2(50)
          CONSTRAINT descripcion_reporte_nn NOT NULL,
     cedulaUsuario_id_fk NUMBER(6)
          CONSTRAINT cedulaUsuario_id_fk_reporte_nn NOT NULL,
          CONSTRAINT cedulaUsuario_id_fk_reporte FOREIGN KEY(cedulaUsuario_id_fk)
               REFERENCES usuario(cedulausuario_id),
     fecha_creacion DATE,
     usuario_creacion VARCHAR2(25),
     fec_ultima_modificacion DATE,
     usuario_ultima_modificacion VARCHAR2(25)   
);
